<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="res" type="at.schauer.gregor.dormancy.scenario.client.ui.ResourceBundle"/>
	<g:HTMLPanel styleName="{res.floatz.layout.box}">
		<g:SimplePanel ui:field="mainPanel" styleName="{res.floatz.layout.l25}  {res.floatz.layout.box}"/>
		<g:FormPanel styleName="{res.floatz.form.form} {res.floatz.layout.r75} {res.floatz.layout.box}">
			<g:HTMLPanel>
				<g:HTMLPanel styleName="{res.floatz.layout.box} {res.floatz.layout.l25} {res.floatz.layout.rspacer}">
					<g:Label text="ID:" styleName="small"/>
					<g:TextBox ui:field="idTextBox" readOnly="true" enabled="false"/>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.floatz.layout.box} {res.floatz.layout.r66} {res.floatz.layout.rspacer}">
					<g:Label text="Name:" styleName="small"/>
					<g:TextBox ui:field="nameTextBox" enabled="false"/>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.floatz.layout.box} {res.floatz.layout.rspacer}">
					<g:Button ui:field="saveButton" text="Save" enabled="false" styleName="{res.floatz.layout.m10}"/>
					<g:Button ui:field="cancelButton" text="Cancel" enabled="false" styleName="{res.floatz.layout.m10}"/>
					<g:Button ui:field="createButton" text="New" styleName="{res.floatz.layout.m10}"/>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:FormPanel>
	</g:HTMLPanel>
</ui:UiBinder>
