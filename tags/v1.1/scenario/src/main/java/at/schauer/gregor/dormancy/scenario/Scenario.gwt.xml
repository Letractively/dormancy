<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN" "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Scenario">

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>
	<inherits name='com.floatzcss.gwt.Css'/>
	<source path="client"/>
	<source path="shared"/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='at.schauer.gregor.dormancy.scenario.client.Scenario'/>

	<!-- Specify the app servlets.                   -->
	<servlet path="/DormancyScenarioService" class="at.schauer.gregor.dormancy.scenario.server.service.DormancyScenarioServiceImpl"/>

</module>
